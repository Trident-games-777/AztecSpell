<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/aztec_nav_graph.xml"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="jp.co.ta.ui.fragments.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_workerFragment"
            app:destination="@id/workerFragment"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_splashFragment_to_aztecGreetingFragment"
            app:destination="@id/aztecGreetingFragment"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/workerFragment"
        android:name="jp.co.ta.ui.fragments.WorkerFragment"
        android:label="fragment_worker"
        tools:layout="@layout/fragment_worker" >
        <argument
            android:name="firebaseData"
            app:argType="jp.co.ta.model.FirebaseData" />
        <argument
            android:name="googleId"
            app:argType="string" />
        <action
            android:id="@+id/action_workerFragment_to_aztecWebFragment"
            app:destination="@id/aztecWebFragment"
            app:popUpTo="@id/workerFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/aztecGreetingFragment"
        android:name="jp.co.ta.ui.fragments.AztecGreetingFragment"
        android:label="fragment_aztec_greeting"
        tools:layout="@layout/fragment_aztec_greeting" >
        <action
            android:id="@+id/action_aztecGreetingFragment_to_aztecCasinoFragment"
            app:destination="@id/aztecCasinoFragment" />
    </fragment>
    <fragment
        android:id="@+id/aztecWebFragment"
        android:name="jp.co.ta.ui.fragments.AztecWebFragment"
        android:label="fragment_aztec_web"
        tools:layout="@layout/fragment_aztec_web" >
        <argument
            android:name="link"
            app:argType="string" />
        <argument
            android:name="domain"
            app:argType="string" />
        <action
            android:id="@+id/action_aztecWebFragment_to_aztecGreetingFragment"
            app:destination="@id/aztecGreetingFragment"
            app:popUpTo="@id/aztecWebFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/aztecCasinoFragment"
        android:name="jp.co.ta.ui.fragments.AztecCasinoFragment"
        android:label="fragment_aztec_casino"
        tools:layout="@layout/fragment_aztec_casino" >
        <action
            android:id="@+id/action_aztecCasinoFragment_to_aztecFinishFragment"
            app:destination="@id/aztecFinishFragment"
            app:popUpTo="@id/aztecGreetingFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/aztecFinishFragment"
        android:name="jp.co.ta.ui.fragments.AztecFinishFragment"
        android:label="fragment_aztec_finish"
        tools:layout="@layout/fragment_aztec_finish" >
        <action
            android:id="@+id/action_aztecFinishFragment_to_aztecGreetingFragment"
            app:destination="@id/aztecGreetingFragment"
            app:popUpTo="@id/aztecFinishFragment"
            app:popUpToInclusive="true" />
    </fragment>
</navigation>